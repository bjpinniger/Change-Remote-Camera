<Extensions>
  <Version>1.6</Version>
  <Panel>
    <PanelId>RemoteCamera</PanelId>
    <Type>InCall</Type>
    <Icon>Camera</Icon>
    <Order>1</Order>
    <Name>Remote Camera</Name>
    <ActivityType>Custom</ActivityType>
    <Page>
      <Name>Remote Camera Control</Name>
      <Row>
        <Name>Set active camera</Name>
        <Widget>
          <WidgetId>camera_0</WidgetId>
          <Name>One</Name>
          <Type>Button</Type>
          <Options>size=1</Options>
        </Widget>
        <Widget>
          <WidgetId>camera_1</WidgetId>
          <Name>Two</Name>
          <Type>Button</Type>
          <Options>size=1</Options>
        </Widget>
        <Widget>
          <WidgetId>camera_2</WidgetId>
          <Name>Three</Name>
          <Type>Button</Type>
          <Options>size=1</Options>
        </Widget>
      </Row>
      <Row>
        <Name>Camera Control</Name>
        <Widget>
          <WidgetId>cameracontrol</WidgetId>
          <Type>DirectionalPad</Type>
          <Options>size=4</Options>
        </Widget>
      </Row>
      <Row>
        <Name>Zoom</Name>
        <Widget>
          <WidgetId>fecc_zoom</WidgetId>
          <Type>Spinner</Type>
          <Options>size=2;style=plusminus</Options>
        </Widget>
        <Widget>
          <WidgetId>widget_2</WidgetId>
          <Type>Spacer</Type>
          <Options>size=1</Options>
        </Widget>
      </Row>
      <PageId>cameracontrol</PageId>
      <Options/>
    </Page>
  </Panel>
</Extensions>
